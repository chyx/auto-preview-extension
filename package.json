{
  "name": "autopreview",
  "displayName": "AutoPreview",
  "publisher": "chyx111",
  "repository": "git@github.com:chyx/auto-preview-extension.git",
  "description": "",
  "version": "0.0.7",
  "engines": {
    "vscode": "^1.55.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:autopreview.helloWorld",
    "onCommand:autopreview.debug",
    "onLanguage:markdown"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "autopreview.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "autopreview.openWikiLinkInPreviousTab",
        "title": "Open current wiki link in the previous tab."
      },
      {
        "command": "autopreview.openWikiLinkInNextTab",
        "title": "Open current wiki link in the next tab."
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.4",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.55.0",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "eslint": "^7.19.0",
    "glob": "^7.1.6",
    "mocha": "^8.2.1",
    "typescript": "^4.1.3",
    "vscode-test": "^1.5.0"
  }
}
